{
  "name": "cyprusjs",
  "version": "2.0.0",
  "private": true,
  "description": "CyprusJS Official Website",
  "homepage": "https://cyprusjs.org/",
  "bugs": {
    "url": "https://github.com/cyprusjs/cyprusjs.org/issues",
    "email": "info@cyprusjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyprusjs/cyprusjs.org"
  },
  "license": "MIT",
  "author": "CyprusJS",
  "contributors": [
    {
      "name": "Patrick Heneise",
      "email": "patrick@zentered.co",
      "url": "https://zentered.co"
    },
    {
      "name": "Jonas Broms",
      "email": "jonas.broms@gmail.com",
      "url": "https://twitter.com/jonasbroms"
    }
  ],
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap",
    "dev": "next dev",
    "export": "next export",
    "lint": "eslint --fix . --ext .js --ext .jsx --ext md --ext mdx",
    "prepare": "husky install",
    "pretty": "prettier --write \"{pages,content}/**/*.{js,jsx,md,mdx,css,scss}\"",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "next": "12.3.1",
    "next-seo": "^5.5.0",
    "next-sitemap": "^3.1.22",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.3",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@netlify/plugin-nextjs": "^4.23.1",
    "eslint": "8.23.1",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "windicss-webpack-plugin": "^1.7.6"
  },
  "engines": {
    "node": "^16",
    "npm": "^7"
  }
}
