{
  "name": "cyprusjs",
  "version": "2.0.0",
  "private": true,
  "description": "CyprusJS Official Website",
  "homepage": "https://cyprusjs.org/",
  "bugs": {
    "url": "https://github.com/cyprusjs/cyprusjs.org/issues",
    "email": "info@cyprusjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyprusjs/cyprusjs.org"
  },
  "license": "MIT",
  "author": "CyprusJS",
  "contributors": [
    {
      "name": "Patrick Heneise",
      "email": "patrick@zentered.co",
      "url": "https://zentered.co"
    },
    {
      "name": "Jonas Broms",
      "email": "jonas.broms@gmail.com",
      "url": "https://twitter.com/jonasbroms"
    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "export": "next export",
    "start": "next start",
    "lint": "eslint --fix . --ext .js --ext .jsx --ext md --ext mdx",
    "pretty": "prettier --write \"{pages,content}/**/*.{js,jsx,md,mdx,css,scss}\"",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "next": "12.0.5",
    "next-seo": "^4.28.1",
    "next-sitemap": "^1.6.203",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^7.1.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@netlify/plugin-nextjs": "^4.23.1",
    "eslint": "7.32.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "windicss-webpack-plugin": "^1.5.8"
  },
  "engines": {
    "node": "^16",
    "npm": "^7"
  }
}
